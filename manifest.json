{
  "author": "ZLGNF",
  "background": {
    "service_worker": "background.js"
  },
  "description": "Unleash channel points and channel rewards on your YouTube™ and Twitch streams, just like how you do on Twitch with our powerful extension!",
  "externally_connectable": {
    "matches": [
      "https://*.youtube.com/*",
      "https://studio.youtube.com/*",
      "https://*.twitch.tv/*"
    ]
  },
  "icons": {
    "56": "icons/ytcr.png"
  },
  "manifest_version": 3,
  "name": "YTCR Revival™",
  "permissions": [
    "activeTab",
    "webNavigation",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "https://*.youtube.com/*",
    "https://studio.youtube.com/*",
    "https://*.twitch.tv/*"
  ],
  "version": "2.0.4",
  "version_name": "2.0.4",
  "web_accessible_resources": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "https://studio.youtube.com/*",
        "https://www.twitch.tv/*"
      ],
      "resources": [
        "commands/*.xml",
        "main.js",
        "reward.css",
        "tailwind.css",
        "icons/*",
        "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css"
      ]
    }
  ]
}
