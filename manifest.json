{
   "author": "Gezel.io",
   "background": {
      "service_worker": "background.js"
   },
   "description": "Unleash channel points and channel rewards on your YouTube™ streams, just like how you do on Twitch with our powerful extension!",
   "externally_connectable": {
      "matches": [
         "https://*.youtube.com/*",
         "https://studio.youtube.com/*"
      ]
   },
   "icons": {
      "56": "icons/ytcr.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "YTCR TEST™",
   "permissions": [
      "activeTab", // Add this permission
      "webNavigation",
      "scripting"
   ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0.4",
   "version_name": "2.0.4",
   "web_accessible_resources": [
      {
         "matches": [
            "https://www.youtube.com/*",
            "https://studio.youtube.com/*"
         ],
         "resources": [
            "main.js",
            "reward.css",
            "tailwind.css",
            "icons/*",
            "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css"
         ]
      }
   ]
}
